<% @textbooks.each do|textbook| %>
<%= form_for ([textbook,textbook.comments.build]) do |f| %>
    
 
    <p>
        <%= f.label :commenters %><br>
        <%= f.text_field :commenters %>
    </p>
    
    <p>
        <%= f.label :body %><br> 
        <%= f.text_area :body %>
    </p>    
    <br>
    <p>
        <%= f.submit %>
    </p>

<% end %>
<% end %>

